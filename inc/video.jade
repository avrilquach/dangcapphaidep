section.heading2.pad30_0
  .container
    .row
      .col-lg-12
        h2 
          span Video
    .row.padtp30_0
      // Carousel Wrapper
      #carousel-thumb.carousel.slide.carousel-fade.carousel-thumbnails(data-ride='carousel')
        // Slides
        .carousel-inner(role='listbox')
          .carousel-item.active
            img.d-block.w-100(src='img/spa/img2.jpg', alt='First slide')
          .carousel-item
            img.d-block.w-100(src='img/spa/img2.jpg', alt='Second slide')
          .carousel-item
            img.d-block.w-100(src='img/spa/img2.jpg', alt='Third slide')
        // /.Slides
        // Controls
        a.carousel-control-prev(href='#carousel-thumb', role='button', data-slide='prev')
          span.carousel-control-prev-icon(aria-hidden='true')
          span.sr-only Previous
        a.carousel-control-next(href='#carousel-thumb', role='button', data-slide='next')
          span.carousel-control-next-icon(aria-hidden='true')
          span.sr-only Next
        // /.Controls
        ol.carousel-indicators
          li.active(data-target='#carousel-thumb', data-slide-to='0')
            img.d-block.w-100(src='https://mdbootstrap.com/img/Photos/Others/Carousel-thumbs/img%20(88).jpg')
          li(data-target='#carousel-thumb', data-slide-to='1')
            img.d-block.w-100(src='https://mdbootstrap.com/img/Photos/Others/Carousel-thumbs/img%20(121).jpg')
          li(data-target='#carousel-thumb', data-slide-to='2')
            img.d-block.w-100(src='https://mdbootstrap.com/img/Photos/Others/Carousel-thumbs/img%20(31).jpg')
      // /.Carousel Wrapper
